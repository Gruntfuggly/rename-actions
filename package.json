{
    "name": "rename-actions",
    "displayName": "Rename Actions",
    "description": "Perform actions when a file is renamed",
    "icon": "icon.png",
    "version": "0.0.1",
    "publisher": "Gruntfuggly",
    "repository": "https://github.com/Gruntfuggly/rename-actions",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Rename Actions",
            "properties": {
                "rename-actions.watchers": {
                    "type": "array",
                    "markdownDescription": "An array of watchers, consisting of a glob to match files and an array of pairs of regex/snippt to locate and replace in the document."
                }
            }
        }
    },
    "devDependencies": {
        "vscode": "^1.0.0"
    }
}