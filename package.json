{
    "name": "rename-actions",
    "displayName": "Rename Actions",
    "description": "Perform actions when a file is renamed",
    "icon": "icon.png",
    "version": "0.0.3",
    "publisher": "Gruntfuggly",
    "repository": "https://github.com/Gruntfuggly/rename-actions",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Rename Actions",
            "properties": {
                "rename-actions.watchers": {
                    "type": "array",
                    "markdownDescription": "An array of watchers, consisting of a glob to match files and an array of glob/regex/snippet to locate and replace in the document."
                },
                "rename-actions.debug": {
                    "type": "boolean",
                    "markdownDescription": "Create a debug channel in the Output view"
                }
            }
        }
    },
    "devDependencies": {
        "vscode": "^1.0.0"
    },
    "__metadata": {
        "id": "cafc6ccf-5109-4e1b-8763-570ed4b130ca",
        "publisherDisplayName": "Gruntfuggly",
        "publisherId": "d4906d2e-f2ee-492d-9c7c-02b6160599ec"
    },
    "dependencies": {
        "micromatch": "^4.0.1"
    }
}